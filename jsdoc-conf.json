{
    "plugins": [
        "jsdoc-jsx",
        "plugins/typescript"
    ],
    "typescript": {
        "rootModuleName": "jsdoc3-examples",
        "outDir": "./typings"
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
